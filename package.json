{
  "name": "cgiar-api",
  "version": "1.0.0",
  "description": "A REST API for technical test",
  "main": "dist/app.js",
  "scripts": {
    "start": "dotenvx run -f .env.production -- node dist/app.js",
    "build": "tsc",
    "debug": "dotenvx run -f .env.development -- node --inspect --require ts-node/register src/app.ts",
    "dev": "dotenvx run -f .env.development -- nodemon src/app.ts",
    "lint": "eslint \"**/*.ts\" --fix --ignore-pattern .gitignore",
    "format": "prettier --write src/**/*.ts --ignore-path .gitignore",
    "test": "dotenvx run -f .env.testing -- jest --coverage",
    "prepare": "husky"
  },
  "keywords": [
    "api",
    "rest",
    "barber",
    "technical",
    "test"
  ],
  "author": "Heyner Humanez",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/pg": "^8.11.6",
    "eslint": "~9.4.0",
    "globals": "^15.4.0",
    "husky": "^9.0.11",
    "nodemon": "^3.1.3",
    "prettier": "3.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.0"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^0.44.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.5.0",
    "morgan": "^1.10.0",
    "zod": "^3.23.8"
  }
}
